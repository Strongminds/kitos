{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "types": [],
    "typeRoots": [
      "./typings-custom",
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "jspm_packages",
    "typings"
