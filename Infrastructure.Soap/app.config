<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="AdresseBinding">
                    <security mode="Transport">
                        <transport clientCredentialType="Certificate" />
                    </security>
                </binding>
                <binding name="BrugerBinding">
                    <security mode="Transport">
                        <transport clientCredentialType="Certificate" />
                    </security>
                </binding>
                <binding name="VirksomhedBinding">
                    <security mode="Transport">
                        <transport clientCredentialType="Certificate" />
                    </security>
                </binding>
                <binding name="PersonBinding">
                    <security mode="Transport">
                        <transport clientCredentialType="Certificate" />
                    </security>
                </binding>
                <binding name="OrganisationBinding">
                    <security mode="Transport">
                        <transport clientCredentialType="Certificate" />
                    </security>
                </binding>
            </basicHttpBinding>
            <customBinding>
                <binding name="Adresse">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
                <binding name="Bruger">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
                <binding name="Person">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="https://localhost:8080/organisation/adresse/6"
                binding="basicHttpBinding" bindingConfiguration="AdresseBinding"
                contract="STSAdresse.AdressePortType" name="AdressePort" />
            <endpoint address="https://localhost:8080/organisation/bruger/6"
                binding="basicHttpBinding" bindingConfiguration="BrugerBinding"
                contract="STSBruger.BrugerPortType" name="BrugerPort" />
            <endpoint address="https://localhost:8080/rganisation/person/6"
                binding="basicHttpBinding" bindingConfiguration="PersonBinding"
                contract="STSPerson.PersonPortType" name="PersonPort" />
            <endpoint address="http://kombit.dk/sts/organisation/adresse"
                binding="customBinding" bindingConfiguration="Adresse" contract="STSAdresse.AdressePortType"
                name="Adresse" />
            <endpoint address="http://kombit.dk/sts/organisation/bruger"
                binding="customBinding" bindingConfiguration="Bruger" contract="STSBruger.BrugerPortType"
                name="Bruger" />
            <endpoint address="http://kombit.dk/sts/organisation/person"
                binding="customBinding" bindingConfiguration="Person" contract="STSPerson.PersonPortType"
                name="Person" />
        </client>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/></startup></configuration>
